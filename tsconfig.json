{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
        "skipLibCheck": true,
        "outDir": "dist",
        "strict": false,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2015",
            "ES2016",
            "ES2018",
            "ES2019.Object",
            "ES2018.AsyncIterable",
            "ES2020.String",
            "ES2019.Array"
       ],
    },
    "include": [
        "./src/**/*"
    ]
}
